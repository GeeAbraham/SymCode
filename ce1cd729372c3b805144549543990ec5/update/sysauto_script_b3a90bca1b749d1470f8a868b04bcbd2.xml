function updateDynamicRecipients(){
	var rec = new GlideRecord('x_ssnbv_symcode_package');
	rec.addEncodedQuery('active=true^latest_versionISNOTEMPTY');
	rec.addActiveQuery('is_email_receiver');
	rec.query();
	while(rec.next())
		{
			var rec1 = new GlideRecord("x_ssnbv_symcode_installed_package_version");
			rec1.addQuery('version.package',rec.sys_id);
			rec1.query();
			if(rec1.next())
				{
					gs.eventQueue("x_ssnbv_symcode.symcode.installed.packag",current,rec.sys_id, rec.owner);
				}

		}
	

}]]></script>
