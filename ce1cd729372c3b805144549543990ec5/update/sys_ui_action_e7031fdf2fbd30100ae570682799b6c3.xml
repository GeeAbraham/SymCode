        <order>100</order>
        <script><![CDATA[//send notifications
var pkg = new GlideRecord('x_ssnbv_symcode_package');
pkg.addQuery('active', true);
pkg.addQuery('owner', '!=', '');
pkg.query();
while (pkg.next()) {
    if (pkg.compatibility.toString().indexOf(current.sys_id) == -1 && pkg.build_version != current.sys_id) {
        if (pkg.sys_id == '27593eea1b0324504d85a713604bcb93')
            gs.eventQueue("x_ssnbv_symcode.symcode.package.notify", pkg, pkg.owner, current.name);
    }
}

if (current.notified != true) {
    current.notified = true;
    current.update();
}

action.setRedirectURL(current);
gs.addInfoMessage('Recertification  notifications have been sent.');]]></script>
